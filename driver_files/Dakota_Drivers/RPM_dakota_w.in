#  DAKOTA INPUT FILE - RPM_test.in
#  Usage:
#  dakota -i RPM_dakota_2.in -o RPM_dakota_2.out > RPM_dakota.stdout

environment
     tabular_data
     tabular_data_file "RPM_dakota_w.dat"

method,
	 bayes_calibration queso 
     metropolis_hastings
	 proposal_covariance prior                          
     chain_samples = 1000 seed = 348
	 export_chain_points_file  'posterior.dat'
	 output debug

variables,
	 uniform_uncertain = 3
	
	 lower_bounds  -3. -8. 0.001
     upper_bounds  1. 0. 10. 
	 descriptor "FR" "K" "Y"


interface,
	fork
	  asynchronous
	  evaluation_concurrency 1

	  failure_capture recover 2
	  
	  parameters_file = "params.in"
	  results_file    = "results.out"
	
	  copy_files "template_dir/*"

	  analysis_driver = "python ./RPM_driver_W.py"


      work_directory
      named "run"
	  directory_tag
      directory_save
      file_save

responses,
	num_response_functions = 1
    response_descriptors "rmse"  #/likelihood
	no_gradients
	no_hessians
