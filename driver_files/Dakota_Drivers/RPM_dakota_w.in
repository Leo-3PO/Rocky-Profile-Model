#  DAKOTA INPUT FILE - RPM_test.in
#  Usage:
#  dakota -i RPM_dakota_w.in -o RPM_dakota_w.out > RPM_dakota.stdout

environment
     tabular_data
     tabular_data_file "RPM_dakota_w.dat"

method,
	 bayes_calibration queso 
     delayed_rejection
	 proposal_covariance prior                          
     chain_samples = 500 seed = 348
	 #burn_in_samples = 100
	 chain_diagnostics confidence_intervals
	 export_chain_points_file  'posterior.dat'
	 logit_transform
	 output debug

variables,
	 normal_uncertain = 3
	 #lower_bounds  0.005 -8 -5
     #upper_bounds  -0.9 -1 -1
	 means -1.15 -2.9 -4.9
	 std_deviations 0.05 0.05 0.05
	 descriptor "FR" "K" "Y"


interface,
	fork
	  asynchronous
	  evaluation_concurrency 1

	  failure_capture recover 99999999
	  
	  parameters_file = "params.in"
	  results_file    = "results.out"
	
	  copy_files "template_dir/*"

	  analysis_driver = "python ./RPM_driver_W.py"


      work_directory
      named "run"
	  directory_tag
      directory_save
      file_save

responses,
	num_response_functions = 1
    response_descriptors "rmse"  #/likelihood
	no_gradients
	no_hessians
