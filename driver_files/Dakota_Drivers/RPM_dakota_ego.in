#  DAKOTA INPUT FILE - RPM_test.in
#  Usage:
#  dakota -i RPM_dakota_ego.in -o RPM_dakota_ego.out > RPM_dakota.stdout

environment
     tabular_data
     tabular_data_file "RPM_dakota_ego.dat"

method,
     efficient_global
	 gaussian_process surfpack
	 seed = 348
	 #samples = ???
	 output debug

variables,
	 normal_uncertain = 2
	 means 5. 2.5
	 std_deviations 0.5 0.25
	 #lower_bounds  0.00000001 0.00000001
     #upper_bounds  10. 5.
	 descriptor "FR" "K"

interface,
	fork
	  asynchronous
	  evaluation_concurrency 1

	  failure_capture recover 2
	  
	  parameters_file = "params.in"
	  results_file    = "results.out"
	
	  copy_files "template_dir/*"

	  analysis_driver = "python ./RPM_driver_2.py"


      work_directory
      named "run"
	  directory_tag
      directory_save
      file_save

	  
responses,
	calibration_terms = 1
    response_descriptors "rmse"  #/likelihood
	no_gradients
	no_hessians
