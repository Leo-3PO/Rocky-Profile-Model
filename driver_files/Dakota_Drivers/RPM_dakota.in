#  DAKOTA INPUT FILE - RPM_test.in
#  Usage:
#  dakota -i RPM_dakota.in -o RPM_dakota.out > RPM_dakota.stdout

environment
  tabular_data
    tabular_data_file "RPM_dakota.dat"

method,
	bayes_calibration queso 
        metropolis_hastings                        #/DRAM?
        samples = 10 seed = 348
	output debug

variables,
	 uniform_uncertain = 2 
	 initial_point 0.02 0.000002
	 lower_bounds  0.00005 0.
         upper_bounds  1. 0.001
	 descriptor "FR" "K"

interface,
	fork
	  asynchronous
	evaluation_concurrency 1
	  analysis_driver = "python ./RPM_driver.py"

	  parameters_file = "params.in"
	  results_file    = "results.out"
	copy_files "template_dir/*"

     work_directory
      named "Queso/run"
      directory_tag
      directory_save
      file_save

	  
responses,
	num_response_functions = 1
        response_descriptors "rmse"  #/likelihood
	no_gradients
	no_hessians
